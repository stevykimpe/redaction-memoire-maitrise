pmid = {21229412},
title = {{Fast 3D reconstruction of the lower limb using a parametric model and statistical inferences and clinical measurements calculation from biplanar X-rays}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21229412 http://www.tandfonline.com/doi/abs/10.1080/10255842.2010.540758},
volume = {15},
